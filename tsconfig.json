{
  "compilerOptions": {
    "target": "ES2022",
    "module": "node16",
    "moduleResolution": "node16",
    "baseUrl": ".",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": ["vite/client", "svelte", "node"],
//    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "emitDeclarationOnly": false,

  //  "outDir": "dist",
    // "sourceMap": true,
    // "declaration": false,
    // "declarationMap": false,
    // "allowJs": false,
    "paths": {
      "@speajus/gazel-proto/*":["./bazel-bin/proto/*", "../proto/*"],
      "@speajus/gazel-proto":["./bazel-bin/proto/index.js", "../proto/index.js"],
      "*/build_pb.js": ["./bazel-bin/proto/_virtual_imports/build_proto/build_pb.js"],
      "*/stardoc_output_pb.js": ["./bazel-bin/proto/_virtual_imports/build_proto/stardoc_output_pb.js"]
    }

  },
  "exclude": [
    "node_modules",
    "dist",
  ]
  
}
