{
  "name": "gazel",
  "version": "1.0.0",
  "description": "Bazel exploration tool with Svelte frontend and Node.js backend",
  "type": "module",
  "scripts": {
    "dev": "$npm_execpath run dev:server & $npm_execpath run dev:client",
    "dev:server": "tsx  server/index.ts",
    "dev:client": "vite",
    "build": "$npm_execpat run build:server && $npm_execpat run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "build:all": "$npm_execpath run build && npm run start",
    "typecheck": "tsc --noEmit && svelte-check",
    "env": "env"
  },
  "dependencies": {
    "@types/highlight.js": "^10.1.0",
    "@xyflow/svelte": "0.1.36",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "elkjs": "^0.11.0",
    "express": "^4.19.2",
    "highlight.js": "^11.11.1",
    "sax": "^1.4.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tsconfig/svelte": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/sax": "^1.2.7",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.0",
    "bits-ui": "^0.21.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.0",
    "lucide-svelte": "^0.395.0",
    "postcss": "^8.4.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "bazel",
    "build",
    "exploration",
    "svelte",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
