{
  "name": "gazel",
  "version": "1.0.0",
  "description": "A modern web-based UI for exploring and understanding Bazel workspaces",
  "type": "module",
  "main": ".vite/build/main.cjs",
  "config": {
    "forge": "./electron/forge.config.ts"
  },
  "scripts": {
    "dev": "$npm_execpath run dev:server & $npm_execpath run dev:client",
    "dev:server": "tsx  server/index.ts",
    "dev:client": "vite",
    "dev:bazel": "./electron/dev-bazel.sh",
    "start": "electron-forge start",
    "start:bazel": "bazel run //electron:start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "generate-icons": "./scripts/generate-icons.sh",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build:finalize": "cp package.prod.json dist/package.json && cp .env.example dist/.env.example && cp README.prod.md dist/README.md && echo 'Production build ready in dist/'",
    "build": "$npm_execpath run build:server && $npm_execpath run build:client && $npm_execpath run build:finalize",
    "build:all": "$npm_execpath run build && npm run start",
    "build:prod": "$npm_execpath run build && echo 'Build complete. To deploy: cd dist && npm install && npm start'",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit && svelte-check",
    "env": "env"
  },
  "dependencies": {
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@connectrpc/connect-web": "^2.1.0",
    "@hpcc-js/wasm": "^2.26.2",
    "@types/highlight.js": "^10.1.0",
    "@xyflow/svelte": "0.1.36",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "elkjs": "^0.11.0",
    "express": "^4.19.2",
    "highlight.js": "^11.11.1",
    "sax": "^1.4.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@bazel/ibazel": "^0.26.10",
    "@bufbuild/protobuf": "^2.9.0",
    "@bufbuild/protoc-gen-es": "^2.9.0",
    "@connectrpc/protoc-gen-connect-es": "^1.7.0",
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tsconfig/svelte": "^5.0.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/sax": "^1.2.7",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.0",
    "bits-ui": "^0.21.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "lucide-svelte": "^0.544.0",
    "postcss": "^8.4.0",
    "protobufjs": "^7.5.4",
    "svelte": "^5.39.8",
    "svelte-check": "^3.8.6",
    "svelte-preprocess": "^6.0.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tar": "^7.5.1",
    "tslib": "^2.6.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2",
    "vite": "^5.4.20"
  },
  "keywords": [
    "bazel",
    "build",
    "exploration",
    "svelte",
    "nodejs",
    "typescript",
    "electron"
  ],
  "author": "",
  "license": "MIT",
  "build": {
    "appId": "com.gazel.app",
    "productName": "Gazel",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "electron/**/*.js",
      "server/**/*.js",
      "client/dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png",
      "category": "Development"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  }
}
